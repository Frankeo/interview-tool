{
  "name": "interview-tool",
  "version": "2.0.0",
  "description": "This repo contains a CLI tool for create, save and deploy exercises for interviews.",
  "bin": "./index.js",
  "private": false,
  "scripts": {
    "ship": "codesandbox src/output/",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint": "eslint ./ --fix",
    "lint:ci": "eslint ./",
    "test": "jest --watch",
    "test:ci": "jest --coverage",
    "husky": "npm run lint:ci && npm run test:ci",
    "husky:ci": "npm run husky && npm run coveralls",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frankeo/interview-challenges.git"
  },
  "keywords": [
    "Interview",
    "Solutions"
  ],
  "author": "Francisco Moreno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Frankeo/interview-challenges/issues"
  },
  "homepage": "https://github.com/Frankeo/interview-challenges#readme",
  "dependencies": {
    "chalk": "4.1.0",
    "codesandbox": "2.1.16",
    "console-table-printer": "2.5.1",
    "execa": "5.0.0",
    "find-up": "5.0.0",
    "fs-extra": "9.0.1",
    "listr": "0.14.3",
    "signale": "1.4.0",
    "sqlite3": "5.0.0",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "commitizen": "4.2.2",
    "coveralls": "3.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.0.6",
    "jest": "27.0.6",
    "pinst": "2.1.1",
    "prettier": "2.2.1"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
